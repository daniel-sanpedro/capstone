{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "render-postbuild": "npm install --prefix client && npm run build --prefix client",
    "install": "npm install --prefix server && npm install --prefix client",
    "dev:backend": "npm start --prefix server",
    "dev:frontend": "npm start --prefix client",
    "build:backend": "npm run build --prefix server",
    "build:frontend": "npm run build --prefix client",
    "start": "node server/index.js",
    "build": "npm run build:backend && npm run build:frontend",
    "dev": "vite",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "vite": "^5.3.1"
  }
}
